msgid "Uncle Milty"
msgstr ""

#: ../fheroes2/resource/maps_text.cpp:601
msgid "What are you doing here?"
msgstr ""

#: ../fheroes2/resource/maps_text.cpp:602
msgid "Do Not Enter!"
msgstr ""

#: ../fheroes2/resource/maps_text.cpp:603
msgid "Turn Back Now!"
msgstr ""

#: ../fheroes2/resource/maps_text.cpp:604
